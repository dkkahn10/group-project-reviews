<div class='wrapper'>
  <h1>User Index Page</h1>

  <div class='users-list'>
    <% @users.each do |user| %>
      <li class='user'><%= user.username %></li>
      <li class = 'delete_user'><%= link_to "Delete #{user.username}", @user, method: :delete, data: { confirm: "You sure?" } %></li>
    <% end %>
  </div>
</div>
